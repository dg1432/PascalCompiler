program test(input, output);

  procedure proc1(x: integer;
      y: real;
      z: array[1 .. 5] of integer);
    var i: integer;
    var j: real;
    var k: array[1 .. 5] of integer;

    begin
      z[3] := x * a - 1;
      call proc2(-3.14, x + 1)
    end;
		
  procedure proc2(a: real; b: integer);
    begin
      if not(a >= 2) then
        a := - a
      else
        if j = -15.3 then
          j := b
        else
          j := 1.23e2
    end;
		
begin
  call proc1(50, 8.35, k)
